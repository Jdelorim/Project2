<div id="profile-form" class="w3-display-container w3-content" style="max-width:7000px;">
  <div class="w3-display-middle w3-padding w3-col l9 m10">
    <div class="w3-container" id="form-heading">
      <h1>{{titlePage}}</h1>
    </div>
        <div class="card-body">
          <img id ="profileImage" class="card-img-top img-fluid" src={{dataInfo.img}} alt="">
          <h1 class="card-title" id= "masseuse-name">Full Name: {{dataInfo.fname}} {{dataInfo.lname}}</h1>
           <h3 class="card-title">Email: {{dataInfo.email}}</h3>
          <h3 class="card-title">Store Name: {{dataInfo.storeName}}</h3>
           <h3 class="card-title">Address: {{dataInfo.address}}</h3>
            <img id ="QRImage" class="card-img-top img-fluid" src={{dataInfo.QRcode}} alt="">
      </div>
    </div>

